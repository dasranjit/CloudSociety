@model CloudSocietyEntities.SocietyMemberJointHolder
@{
    ViewBag.Title = "Member Joint Holder Details";
    ViewBag.Header = "Member Joint Holder Details";
    ViewBag.SocietyNameHeader = ViewBag.SocietyHead;
    Layout = "~/Views/Shared/SocietyLayout.cshtml";    
    var societyMember = (CloudSocietyEntities.SocietyMember)ViewBag.SocietyMember;
    bool IsFullAccess = new CloudSociety.Controllers.AppInfoController().IsFullAccess();
}
<span class="SocName">@societyMember.Member</span>
<br />
<br />
<fieldset style="width: 500px">
    <legend>Member Joint Holder Details</legend>
    <table border="0" cellpadding="0" cellspacing="0">        
        <tr>
            <td>
                Name :
            </td>
            <td>
                @Html.DisplayFor(model => model.Name)
            </td>
        </tr>
         <tr>
            <td>
                Member Class :
            </td>
            <td>
                @Html.DisplayFor(model => model.MemberClass.Class)
            </td>
        </tr>
          <tr>
            <td>
                Start Date :
            </td>
            <td>
                @String.Format("{0:dd-MMM-yyyy}",Model.StartDate)
            </td>
        </tr>
        <tr>
            <td>
                End Date :
            </td>
            <td>
                @String.Format("{0:dd-MMM-yyyy}", Model.EndDate)
            </td>
        </tr>
   <tr>
            <td>
                Enterance Fee Paid On :
            </td>
            <td>
                 @String.Format("{0:dd-MMM-yyyy}",Model.EnteranceFeePaidOn)
            </td>
        </tr>
        <tr>
            <td>
                Address :
            </td>
            <td>
                 @Html.DisplayFor(model => model.Address)
            </td>
        </tr>
    <tr>
            <td>
                City :
            </td>
            <td>
                 @Html.DisplayFor(model => model.City)
            </td>
        </tr>
        <tr>
            <td>
                PIN :
            </td>
            <td>
                 @Model.PIN
            </td>
        </tr>
    <tr>
            <td>
                State :
            </td>
            <td>
                 @(Model.StateID == null ? "" : Model.State.Name)
            </td>
        </tr>
         <tr>
            <td>
                Country :
            </td>
            <td>
                @(Model.StateID == null ? "" : Model.State.Country.Name)
            </td>
        </tr>
         <tr>
            <td>
                Phone No :
            </td>
            <td>
                 @Html.DisplayFor(model => model.PhoneNo)
            </td>
        </tr>
          <tr>
            <td>
                Office Phone No :
            </td>
            <td>
                 @Html.DisplayFor(model => model.OfficePhoneNo)
            </td>
        </tr>
       <tr>
            <td>
                Mobile No :
            </td>
            <td>
                 @Html.DisplayFor(model => model.MobileNo)
            </td>
        </tr>
         <tr>
            <td>
                EMail Id :
            </td>
            <td>
                 @Html.DisplayFor(model => model.EMailId)
            </td>
        </tr>
  </table>
</fieldset>
<div>
@if (IsFullAccess)
            {
    @Html.ActionLink("Edit", "Edit", new { id = Model.SocietyMemberJointHolderID, societySubscriptionID = ViewBag.SocietySubscriptionID }, new { @class = "EditDelete" })
    <text>|</text>
    }
    @Html.ActionLink("Back to List", "Index", new { societyMemberID = Model.SocietyMemberID, societySubscriptionID = ViewBag.SocietySubscriptionID }, new { @class = "EditDelete" })
</div>
