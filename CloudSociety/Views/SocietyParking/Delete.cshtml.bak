@model CloudSocietyEntities.SocietyParking
@{
    ViewBag.Title = "Delete Parking";
    ViewBag.Header = "Parking";
    ViewBag.SocietyNameHeader = ViewBag.SocietyHead;
    Layout = "~/Views/Shared/SocietyLayout.cshtml";
    bool IsFullAccess = new CloudSociety.Controllers.AppInfoController().IsFullAccess();
}
<div class="LabelDel">
    Are you sure you want to delete this?
</div>
<br />
@using (Html.BeginForm())
{
    @Html.HiddenFor(model=>model.ParkingNo)
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")        
    <fieldset style="width: 400px">
        <legend>Parking Details</legend>
        <table>
            <tr>
                <td>
                    Parking No. :
                </td>
                <td>
                    @Html.DisplayFor(model => model.ParkingNo)
                </td>
            </tr>
            <tr>
                <td>
                    Parking Type :
                </td>
                <td>
                    @Html.DisplayFor(model => model.ParkingType.Type)
                </td>
            </tr>
            @if (IsFullAccess)
            {
            <tr>
                <td align="center" colspan="2">
                    <input type="submit" value="Delete" class="buttton" style="width: 45px; height: 23px;" />
                </td>
            </tr>
            }
        </table>
    </fieldset>   
}
<div>
    @Html.ActionLink("Back to List", "Index", new { id = (Guid)ViewBag.SocietySubscriptionID }, new { @class = "EditDelete" })
</div>
