@{
    ViewBag.Title = "Generate Collection Report";
    ViewBag.SocietyNameHeader = ViewBag.SocietyHead;
    Layout = "~/Views/Shared/SocietyLayout.cshtml";
}
<br />
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")
    <center>
        <fieldset style="width: 420px">
            <br />
            <table>
                <tr>
                    <td colspan="2" align="center">
                        <div class="LabelDel">
                            This will Generate PDF for Collection Report<br />
                            OK to Download.</div>
                    </td>
                </tr>
                <tr>
                    <td align="center">
                        <input type="submit" value="OK" class="buttton" style="width: 45px; height: 23px;" />
                    </td>
                    <td align="center">
                        @Html.ActionLink("Cancel", "Menu", "SocietySubscription", new { id = ViewBag.SocietySubscriptionID }, new { @class = "buttton", style = "width: 50px; height: 23px;" })
                    </td>
                </tr>
            </table>
        </fieldset>
    </center>
}
