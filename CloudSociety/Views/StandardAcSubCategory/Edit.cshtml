@model CloudSocietyEntities.StandardAcSubCategory
@{
    ViewBag.Title = "Edit";
    ViewBag.Header = " Standard AcSubCategory";
    
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")
    <fieldset style="width: 600px">
        <legend>Standard AcSubCategory</legend>
        <br />
        <table border="0" cellpadding="0" cellspacing="0">           
                         
            <tr>
                <td class="editor-label">
                    @Html.Label("Category :")
                </td>
                <td class="editor-field">
                    @Html.DropDownList("CategoryID", new SelectList(ViewBag.StandardAC as System.Collections.IEnumerable, "CategoryID", "Category"), "<- Select Standard Account Category ->")
                    @Html.ValidationMessageFor(model => model.CategoryID)
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.Label("Sub-Category :")
                </td>
                <td>
                    @Html.EditorFor(model => model.SubCategory)<span class="editor-field"> @Html.ValidationMessageFor(model => model.SubCategory)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.Sequence) :
                </td>
                <td>
                    @Html.TextBox("Sequence", Model.Sequence, new { @class = "shortWidth" })
                    @*<input type="text" name="Sequence" size="10" value=@Model.Sequence></input>@Html.EditorFor(model => model.Sequence)*@
                    <span class="editor-field">@Html.ValidationMessageFor(model => model.Sequence)</span>
                </td>
            </tr>
            <tr>
                <td colspan="2" align="center">
                    <input type="submit" value="Save" class="buttton" style="width: 45px; height: 23px;
                        vertical-align: middle;" />
                </td>
            </tr>
        </table>
         @Html.HiddenFor(model => model.SubCategoryID) 
        @Html.HiddenFor(model => model.CreatedByID)
        @Html.HiddenFor(model => model.CreatedOn)
    </fieldset>
}
<div>
    @*@Html.ActionLink("Back to List", "Index", null, new { @class = "EditDelete" })*@
    <a href="@ViewBag.BackUrl" class="EditDelete">Back to List</a>
</div>
