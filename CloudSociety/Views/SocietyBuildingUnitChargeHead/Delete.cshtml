@model CloudSocietyEntities.SocietyBuildingUnitChargeHead
@{
    ViewBag.Title = "Delete Society Building Unit Charge Head";
    ViewBag.Header = " Society Building Unit Charge Head";
    ViewBag.SocietyNameHeader = ViewBag.SocietyHead;
    Layout = "~/Views/Shared/SocietyLayout.cshtml";
    var societyBuildingUnit = (CloudSocietyEntities.SocietyBuildingUnit)ViewBag.SocietyBuildingUnit;
    bool IsFullAccess = new CloudSociety.Controllers.AppInfoController().IsFullAccess();
}
<div class="LabelDel">
    Are you sure you want to delete this?
</div>
<br />
@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.Rate)
    @Html.ValidationSummary(false, "Following errors occurred while processing your request") 
    <fieldset style="width: 500px">
        <legend>Society Building Unit Charge Head</legend>
        <table>
            <tr>
                <td>
                    Building Unit :
                </td>
                <td>
                    @Html.DisplayFor(model => model.SocietyBuildingUnit.Unit)
                </td>
            </tr>
            <tr>
                <td>
                    Charge Head :
                </td>
                <td>
                    @ViewBag.SocietyChargeHead
                </td>
            </tr>
            <tr>
                <td>
                    Rate :
                </td>
                <td>
                    @Html.DisplayFor(model => model.Rate)
                </td>
            </tr>
            @if (IsFullAccess)
            {
                <tr>
                    <td align="center" colspan="2">
                        <input type="submit" value="Delete" class="buttton" style="width: 45px; height: 23px;" />
                    </td>
                </tr>
            }
        </table>
    </fieldset>
    @Html.HiddenFor(m => m.SocietyBuildingUnitChargeHeadID)
    @Html.Hidden("societySubscriptionID", (Guid)ViewBag.SocietySubscriptionID)
}
<div>
    @Html.ActionLink("Back to List", "Index", new { societyBuildingUnitID = Model.SocietyBuildingUnitID, societySubscriptionID = (Guid)ViewBag.SocietySubscriptionID }, new { @class = "EditDelete" })
</div>
