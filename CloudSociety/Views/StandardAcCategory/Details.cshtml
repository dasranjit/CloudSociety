@model CloudSocietyEntities.StandardAcCategory

@{
    ViewBag.Title = "Details";
    ViewBag.Header = "Details";
 
}

<fieldset style="width: 600px">
   <legend>StandardAcCategory</legend>
  <br />
    <table border="0" cellpadding="0" cellspacing="0">
        <tr>
            <td class="editor-label"  >
                Category :
            </td>
            <td>
                @Model.Category
            </td>
        </tr>
         <tr>
            <td class="editor-label"  >
                DrCr :
            </td>
            <td>
                 @(Model.DrCr == "D" ? "Debit" : (Model.DrCr == "C" ? "Credit" : ""))
            </td>
        </tr>
         <tr>
            <td class="editor-label"  >
                Nature :
            </td>
            <td>
                @(Model.Nature == "I" ? "Income Expenditure" : (Model.Nature == "B" ? "Balnce Sheet" : ""))
            </td>
        </tr>
         <tr>
            <td class="editor-label"  >
                Sequence :
            </td>
            <td>
                @Model.Sequence
            </td>
        </tr>
       
    </table>
   

</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.CategoryID }, new { @class = "EditDelete" }) |
    @Html.ActionLink("Back to List", "Index", null, new { @class = "EditDelete" })
</p>

