@model CloudSocietyEntities.StandardAcCategory
@{
    ViewBag.Title = "Edit Standard AcCategory";
    ViewBag.Header = " Standard AcCategory"; 
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")
    <fieldset style="width: 600px">
        <legend>Edit Standard AcCategory</legend>
        @Html.HiddenFor(model => model.CategoryID)
        <br />
        <table border="0" cellpadding="0" cellspacing="0">
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.Category) :
                </td>
                <td>
                    @Html.EditorFor(model => model.Category)<span class="editor-field"> @Html.ValidationMessageFor(model => model.Category)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.DrCr) :
                </td>
                <td>
                    @Html.DropDownList("DrCr", ViewBag.DrCrList as SelectList)<span class="editor-field">
                        @Html.ValidationMessageFor(model => model.DrCr)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.Nature) :
                </td>
                <td>
                    @Html.DropDownList("Nature", ViewBag.NatureList as SelectList)<span class="editor-field">
                        @Html.ValidationMessageFor(model => model.Nature)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.Sequence) :
                </td>
                <td>
                    <input type="text" name="Sequence" size="10" value=@Model.Sequence></input>@*@Html.EditorFor(model => model.Sequence)*@
                    <span class="editor-field">@Html.ValidationMessageFor(model => model.Sequence)</span>
                </td>
            </tr>
            <tr>
                <td colspan="2">
                    <input type="submit" value="Save" class="buttton" style="width: 45px; height: 23px;
                        vertical-align: middle;" />
                </td>
            </tr>
        </table>
        @Html.HiddenFor(model => model.CreatedByID)
        @Html.HiddenFor(model => model.CreatedOn)
    </fieldset>
}
<div>
    @Html.ActionLink("Back to List", "Index", null, new { @class = "EditDelete" })
</div>
