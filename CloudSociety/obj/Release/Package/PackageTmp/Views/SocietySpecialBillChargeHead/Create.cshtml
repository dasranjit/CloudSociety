@model CloudSocietyEntities.SocietySpecialBillChargeHead
@{
    ViewBag.Title = "Create Special Bill Charge Head";
    ViewBag.Header = "Special Bill Charge Head";
    ViewBag.SocietyNameHeader = ViewBag.SocietyHead;
    var societySpecialBill = (CloudSocietyEntities.SocietySpecialBill)ViewBag.SocietySpecialBill;
    Layout = "~/Views/Shared/SocietyLayout.cshtml";
    bool IsFullAccess = new CloudSociety.Controllers.AppInfoController().IsFullAccess();
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<br />
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false, "Following errors occurred while processing your request") 
    <fieldset style="width: 450px">
        <legend>Special Bill Charge Head</legend><br />
        <table border="0" cellpadding="0" cellspacing="0">
            @Html.Hidden("SocietySpecialBillID", societySpecialBill.SocietySpecialBillID)
            <tr>
                <td>
                    @Html.Label("Charge Head :")
                </td>
                <td>
                    @Html.DropDownList("ChargeHeadID", new SelectList(ViewBag.SocietyChargeHeadList as System.Collections.IEnumerable, "ChargeHeadID", "ChargeHead"), "<- Select Charge Head ->")
                    <span class="editor-field">@Html.ValidationMessageFor(model => model.ChargeHeadID)</span>
                </td>
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.Rate) :
                </td>
                <td>
                    @Html.TextBox("Rate", null, new { @class = "shortWidth" })
                    <span class="editor-field">@Html.ValidationMessageFor(model => model.Rate)</span>
                </td>
            </tr>
            @if (IsFullAccess)
            {
            <tr>
                <td colspan="2" align="center">
                    <input type="submit" value="Create" class="buttton" style="width: 45px; height: 23px;" />
                </td>
            </tr>
            }
        </table>
    </fieldset>
}
<div>
    @Html.ActionLink("Back to List", "Index", new { societySpecialBillID = societySpecialBill.SocietySpecialBillID, societySubscriptionID = ViewBag.SocietySubscriptionID }, new { @class = "EditDelete" })
</div>
