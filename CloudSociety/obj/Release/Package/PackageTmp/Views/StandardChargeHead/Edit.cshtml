@model CloudSocietyEntities.StandardChargeHead
@{
    ViewBag.Title = "Edit Standard Charge Head";
    ViewBag.Header = " Standard Charge Head";    
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")
    <fieldset style="width: 580px">
        <legend>Edit Standard Charge Head</legend>
        @Html.HiddenFor(model => model.ChargeHeadID)
        <br />
        <table border="0" cellpadding="0" cellspacing="0">
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.ChargeHead) :
                </td>
                <td>
                    @Html.EditorFor(model => model.ChargeHead)<span class="editor-field"> @Html.ValidationMessageFor(model => model.ChargeHead)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.ChargeInterest) :
                </td>
                <td>
                    @Html.EditorFor(model => model.ChargeInterest)<span class="editor-field"> @Html.ValidationMessageFor(model => model.ChargeInterest)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.AcHeadID) :
                </td>
                <td class="editor-field">
                    @Html.DropDownList("AcHeadID", new SelectList(ViewBag.StandardCh as System.Collections.IEnumerable, "AcHeadID", "AcHead"), "<- Select Account Head ->")
                    @Html.ValidationMessageFor(model => model.AcHeadID)
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.Nature) :
                </td>
                <td>
                    @Html.DropDownList("Nature", ViewBag.NatureList as SelectList, "<- Select Nature ->")<span
                        class="editor-field"> @Html.ValidationMessageFor(model => model.Nature)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.Sequence) :
                </td>
                <td>
                    @Html.TextBox("Sequence", Model.Sequence, new { @class = "shortWidth" })
                    <span class="editor-field">@Html.ValidationMessageFor(model => model.Sequence)</span>
                </td>
            </tr>
            <tr>
                <td colspan="2" align="center">
                    <input type="submit" value="Save" class="buttton" style="width: 45px; height: 23px;
                        vertical-align: middle;" />
                </td>
            </tr>
        </table>
        @Html.HiddenFor(model => model.CreatedByID)
        @Html.HiddenFor(model => model.CreatedOn)
    </fieldset>
}
<div>
    @* @Html.ActionLink("Back to List", "Index", null, new { @class = "EditDelete" })*@
    <a href="@ViewBag.BackUrl" class="EditDelete">Back to List</a>
</div>
