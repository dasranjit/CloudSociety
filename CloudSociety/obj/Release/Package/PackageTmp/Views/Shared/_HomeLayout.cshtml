@{ 
    Layout = null; 
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
     @*<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jqueryslidemenu.css")" />*@
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/leftmenu.css")" />
   <link  rel="icon" href="@Url.Content("~/Content/Images/c1-favicon.gif")" type="image/ico"/>
    <link href="@Url.Content("~/Content/cloud.css")" rel="stylesheet" type="text/css" />
    <style type="text/css">
        body
        {
            margin-left: 0px;
            margin-top: 0px;
            margin-right: 0px;
            margin-bottom: 0px;
            background-image: url(images/bg-back.jpg);
            background-repeat: repeat-x;
            background-color: #e9e9e9;
        }
    </style>
    <style type="text/css">
        a:link
        {
            text-decoration: none;
        }
        a:visited
        {
            text-decoration: none;
        }
        a:hover
        {
            text-decoration: none;
        }
        a:active
        {
            text-decoration: none;
        }
    </style>
   
</head>
<body>
    <table width="969" border="0" align="center" cellpadding="0" cellspacing="0" background="@Url.Content("~/Content/Images/bg.gif")">
        <tr>
            <td align="center" valign="top">
                <table width="922" border="0" align="center" cellpadding="0" cellspacing="0" bgcolor="#FFFFFF">
                    <tr>
