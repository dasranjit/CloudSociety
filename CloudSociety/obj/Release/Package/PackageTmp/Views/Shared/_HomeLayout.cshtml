                                            <ul>
                                                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                                <li>@Html.ActionLink("Product Features", "Productfeature", "Home")</li>
                                                <li>@Html.ActionLink("Product Benefits", "Productbenefits", "Home")</li>
                                                <li>@Html.ActionLink("Services", "Services", "Home")</li>
                                                <li>@Html.ActionLink("Knowledge Centre", "CloudComputing", "Home")</li>
                                            </ul>
                                        </div>
                                    </td>
                                </tr>
                            </table>
                            @RenderBody()
                            <table width="922" height="1" border="0" cellpadding="0" cellspacing="0">
                                <tr>
                                    <td width="906" height="13">
                                    </td>
                                </tr>
                                <tr>
                                    <td height="1" bgcolor="#F0F0F0">
