@model CloudSocietyEntities.StandardAcHead
@{
    ViewBag.Title = "Create AcHead";
    ViewBag.Header = "Standard AcHead";    
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")
    <fieldset style="width: 600px">
        <legend>Create Standard AcHead</legend>
        <br />
        <table border="0" cellpadding="0" cellspacing="0">
            <tr>
                <td class="editor-label">
                    @Html.Label("Account Head :")
                </td>
                <td>
                    @Html.EditorFor(model => model.AcHead)<span class="editor-field">
                        @Html.ValidationMessageFor(model => model.AcHead)
                    </span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.Label("SubCategory :")
                </td>
                <td class="editor-field">
                    @Html.DropDownList("SubCategoryID", new SelectList(ViewBag.StandardAcSubCategory as System.Collections.IEnumerable, "SubCategoryID", "SubCategory"), "<- Select Standard Account Sub Category ->")
                    @Html.ValidationMessageFor(model => model.SubCategoryID)
                </td>
            </tr>
            <tr>
                <td class="editor-label" >
                    @Html.LabelFor(model => model.Sequence) :
                </td>
                <td>
                   <input type="text" name="Sequence" size="10" value="" ></input>@*@Html.EditorFor(model => model.Sequence)*@<span class="editor-field">
                        @Html.ValidationMessageFor(model => model.Sequence)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.LabelFor(model => model.Nature) :
                </td>
                <td>
                    @Html.DropDownList("Nature", ViewBag.NatureList as SelectList, "<- Select Nature ->")<span
                        class="editor-field"> @Html.ValidationMessageFor(model => model.Nature)</span>
                </td>
            </tr>
            <tr>
                <td class="editor-label" >
                    @Html.Label("TDS Category :")
                </td>
                <td class="editor-field">
                    @Html.DropDownList("TDSCategoryID", new SelectList(ViewBag.TDSCategory as System.Collections.IEnumerable, "TDSCategoryID", "Category"), "<- Select TDS Category ->")
                    @Html.ValidationMessageFor(model => model.TDSCategoryID)
                </td>
            </tr>
            <tr>
                <td class="editor-label">
                    @Html.Label("TDS Company :")
                </td>
                <td>
                    @Html.EditorFor(model => model.TDSCompany)<span class="editor-field">
                        @Html.ValidationMessageFor(model => model.TDSCompany)</span>
                </td>
            </tr>
            <tr>
                <td colspan="2" align="center">
                    <input type="submit" value="Create" class="buttton" style="width: 45px; height: 23px;
                        vertical-align: middle;" />
                </td>
            </tr>
        </table>
    </fieldset>
}
<div>
    @Html.ActionLink("Back to List", "Index", null, new { @class = "EditDelete" })
</div>
