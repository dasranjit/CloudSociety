@model CloudSocietyEntities.StandardAcSubCategory
@{
    ViewBag.Title = "Delete Standard Account Sub-Category";
    ViewBag.Header = "Standard Account Sub-Category";    
}
<div class="LabelDel">
    Are you sure you want to delete this?</div>
<br />
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")
    @Html.HiddenFor(model => model.SubCategoryID)
<fieldset style="width: 500px">

    <legend>Standard Account Sub-Category</legend>
    <br />
    <table border="0" cellpadding="0" cellspacing="0">
     <tr>
            <td>
                Sequence :
            </td>
            <td>
                @Model.Sequence
                @Html.HiddenFor(model => model.Sequence)
            </td>
        </tr>
        <tr>
             <td>
              Sub-Category :
            </td>
            <td>
                @Model.SubCategory
                 @Html.HiddenFor(model => model.SubCategory)
            </td>
        </tr>           
    <tr>
             <td>
                Category :
            </td>
            <td>
             @(Model.StandardAcCategory != null ? Model.StandardAcCategory.Category : "")
             @Html.HiddenFor(model => model.CategoryID)
            </td>
        </tr>     
        <tr>
             <td>
                Part Deatails :
            </td>
            <td>
            @if (Model.PartDetails == true)
                    {
                        <text>
                        <img src="@Url.Content("~/Content/Images/tick-green.png")"  />
                        </text> 
                    }
                    else
                    { 
                        <text>
                        <img src="@Url.Content("~/Content/Images/Copy of cancel-icon.png")" />
                        </text>
                    }               
                @Html.HiddenFor(model => model.PartDetails)
            </td>
        </tr>
         <tr>
                <td align="center" colspan="2">
                    <input type="submit" value="Delete" class="buttton" style="width: 45px; height: 23px;
                        vertical-align: middle;" />
                </td>
            </tr>                                           
    </table>
</fieldset>
@Html.HiddenFor(model => model.CreatedByID)
@Html.HiddenFor(model => model.CreatedOn)
@Html.HiddenFor(model => model.UpdatedByID)
@Html.HiddenFor(model => model.UpdatedOn)
}
<br />
    <div>   
    <a href="@ViewBag.BackUrl" class="EditDelete">Back to List</a>
</div>



