@model CloudSocietyEntities.AcSubCategory
@{
    ViewBag.Title = "Delete Account Sub-Category";
    ViewBag.Header = "Account Sub-Category";
    ViewBag.SocietyNameHeader = ViewBag.SocietyHead;
    Layout = "~/Views/Shared/SocietyLayout.cshtml";
    bool IsFullAccess = new CloudSociety.Controllers.AppInfoController().IsFullAccess();
}
<div class="LabelDel">
    Are you sure you want to delete this?
</div>
<br />
@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.SubCategory)
    @Html.ValidationSummary(false, "Following errors occurred while processing your request")    
    <fieldset style="width: 500px">
        <legend>Account Sub-Category Details</legend>
        <table>
            <tr>
                <td>
                    Sub-Category :
                </td>
                <td>
                    @Html.DisplayFor(model => model.SubCategory)
                </td>
            </tr>
            <tr>
                <td>
                    Category :
                </td>
                <td>
                    @Html.DisplayFor(model => model.AcCategory.Category)
                </td>
            </tr>
            <tr>
                <td>
                    Part Details :
                </td>
                <td>
                    @if (Model.PartDetails == true)
                    { <text>
                        <img alt="" src="@Url.Content("~/Content/Images/tick-green.png")"  />
                        </text> 
                    }
                    else
                    { 
                        <text>
                        <img alt="" src="@Url.Content("~/Content/Images/Copy of cancel-icon.png")" /></text> 
                    }
                </td>
            </tr>
            <tr>
                <td>
                    Sequence
                </td>
                <td>
                    @Html.DisplayFor(model => model.Sequence)
                </td>
            </tr>
            <tr>
                <td>
                    Active
                </td>
                <td>
                    @if (Model.Active == true)
                    { <text>
                        <img alt="" src="@Url.Content("~/Content/Images/tick-green.png")"  />
                        </text> 
                    }
                    else
                    { 
                        <text>
                        <img alt="" src="@Url.Content("~/Content/Images/Copy of cancel-icon.png")" /></text> 
                    }
                </td>
            </tr>
            @if (IsFullAccess)
            {
                      
                <tr>
                    <td align="center" colspan="2">
                        <input type="submit" value="Delete" class="buttton" style="width: 45px; height: 23px;" />
                    </td>
                </tr>
            }
        </table>
    </fieldset>
}
<div>
    @*@Html.ActionLink("Back to List", "Index", new { id = ViewBag.SocietySubscriptionID }, new { @class = "EditDelete" })*@
    <a href="@ViewBag.BackUrl" class="EditDelete">Back to List</a>
</div>
